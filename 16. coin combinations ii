#include <bits/stdc++.h>
using namespace std;

int main(){
  int n, x;
  cin >> n >> x;
  vector<int> c (n);
  for (int i = 0; i < n; i++) cin >> c[i];
  vector<long long> dp (x + 1, 0);
  dp[0] = 1;
  for (int i = 0; i < n; i++){
    for (int j = 1; j <= x; j++)
      if (j >= c[i]) dp[j] = (dp[j] + dp[j - c[i]])%1000000007;
  }
  cout << dp[x];
}
