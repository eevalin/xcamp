def works(mid):
  count = 0
  for x in l:
    count += x//mid
  return count >= k

n, k = [int(x) for x in input().split()]
l = [float(x) for x in input().split()]
l.sort()
left = 0
right = l[-1]
while left < right:
  mid = (left + right)/2
  if works(mid):
    left = mid + 0.01
  else:
    right = mid - 0.01
  #print(mid)
left -= 0.005
print(f"{left:.2f}")
