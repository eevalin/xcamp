#include <bits/stdc++.h>
using namespace std;

int main(){
  int n, x;
  cin >> n >> x;
  vector<long long> c (n);
  for (int i = 0; i < n; i++) cin >> c[i];
  vector<long long> dp(x + 1, LLONG_MAX);
  dp[0] = 0;
  for (long long val : c){
    for (int j = 0; j <= x - val; j++){
      if (dp[j] != LLONG_MAX){
        dp[j+val] = min(dp[j + val], dp[j] + 1);
      }
    }
  }
  if (dp[x] == LLONG_MAX) cout << -1;
  else cout << dp[x];
}
